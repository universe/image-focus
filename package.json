{
  "version": "3.0.0",
  "description": "A dependency free utility for cropping images based on a focal point ~2.13kB gzipped",
  "keywords": [
    "responsive",
    "image",
    "focus",
    "crop",
    "cropper",
    "focal",
    "focuspoint"
  ],
  "author": "Kevin Kipp <kevin.kipp@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/third774/image-focus"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build --format cjs,esm,umd"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@universe/image-focus",
  "module": "dist/image-focus.esm.js",
  "devDependencies": {
    "@rollup/plugin-image": "^2.0.5",
    "@types/resize-observer-browser": "^0.1.7",
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "blurhash": "^1.1.5"
  }
}