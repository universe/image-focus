{
  "version": "3.3.0",
  "description": "A dependency free utility for cropping images based on a focal point ~2.13kB gzipped",
  "keywords": [
    "responsive",
    "image",
    "focus",
    "crop",
    "cropper",
    "focal",
    "focuspoint"
  ],
  "author": "Kevin Kipp <kevin.kipp@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/third774/image-focus"
  },
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup src/index.ts src/runtime.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "lint": "eslint '**/*.ts' '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' '**/*.ts' --fix"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@universe/image-focus",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./runtime": {
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-image": "^2.0.5",
    "@swc/core": "^1.3.24",
    "@types/resize-observer-browser": "^0.1.7",
    "husky": "^4.2.5",
    "tslib": "^2.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-preact": "^1.1.4",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "dependencies": {
    "blurhash": "^1.1.5"
  }
}
